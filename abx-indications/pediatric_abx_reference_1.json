{
  "metadata": {
    "name": "Pediatric Antibiotic Indication Reference",
    "version": "1.0.0",
    "base_source": "Chua et al. BMJ 2019",
    "description": "ICD-10 antibiotic indication classification with pediatric inpatient modifications",
    "categories": {
      "A": "Always appropriate - antibiotic indicated",
      "S": "Sometimes appropriate - clinical judgment needed",
      "N": "Never appropriate - antibiotic not indicated",
      "P": "Prophylaxis indication",
      "FN": "Febrile neutropenia"
    }
  },
  "pediatric_inpatient_overrides": {
    "R78.81": {
      "category": "A",
      "rationale": "Bacteremia requires treatment in inpatient setting"
